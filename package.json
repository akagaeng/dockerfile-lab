{
  "name": "dockerfile-lab",
  "version": "1.0.0",
  "description": "Best practices for writing Dockerfiles with TypeScript + Express Simple API Server",
  "main": "src/app.ts",
  "scripts": {
    "start": "node app.js",
    "build": "tsc --project ./tsconfig.json",
    "dev": "cross-env NODE_ENV=development ts-node --transpile-only src/app.ts",
    "dev:nodemon": "cross-env NODE_ENV=development nodemon",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akagaeng/dockerfile-lab.git"
  },
  "author": "ksyang <akagaeng@gmail.com> (https://github.com/akagaeng)",
  "homepage": "https://github.com/akagaeng/dockerfile-lab#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "hpp": "^0.2.3",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/hpp": "^0.2.2",
    "@types/node": "^18.11.4",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "cross-env": "^7.0.3",
    "eslint": "8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
